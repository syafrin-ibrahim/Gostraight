{
  "expo": {
    "name": "Gostraight",
    "slug": "Gostraight",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package" : "com.gostraight",
      "config" : {
        "googleMaps" : {
          "apiKey" : "AIzaSyA_FpxYMHefUuxjdoH3z_xJAWKrNyhT3O8"
        },
      "permissions" : ["ACCESS_COARSE_LOCATION","ACCESS_FINE_LOCATION","FOREGROUND_SERVICES"]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
